<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="617">
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/image"
                motion:percentX="0.3"
                motion:percentY="0.3" />
            <KeyPosition
                motion:framePosition="47"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/image"
                motion:percentX="0.6" />
            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/image2"
                motion:percentX="0.3"
                motion:percentY="0.3" />
            <KeyPosition
                motion:framePosition="47"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/image2"
                motion:percentX="0.6" />
            <KeyPosition
                motion:framePosition="47"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/textView"
                motion:percentX="-1"
                motion:percentY="0.2" />
            <KeyPosition
                motion:framePosition="47"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/textView2"
                motion:percentX="-1"
                motion:percentY="0.2" />
            <KeyAttribute
                motion:motionTarget="@+id/image"
                motion:framePosition="61"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/image2"
                motion:framePosition="61"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/image2"
                motion:framePosition="59"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/image"
                motion:framePosition="59"
                android:alpha="0" />
        </KeyFrameSet>
        <OnSwipe
            motion:dragDirection="dragDown"
            motion:maxAcceleration="0.1"
            motion:maxVelocity="2.46"
            motion:springDamping="11.194"
            motion:springMass="4.0"
            motion:springStiffness="522.69"
            motion:touchAnchorId="@+id/back"
            motion:touchAnchorSide="bottom" />
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/arrow_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/image"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintStart_toEndOf="@+id/arrow_back"
            motion:layout_constraintTop_toBottomOf="@+id/arrow_back"
            motion:transitionEasing="accelerate"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="0dp"
            motion:layout_constraintStart_toEndOf="@+id/image"
            motion:layout_constraintTop_toTopOf="@+id/image" />
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="8dp"
            motion:layout_constraintBottom_toBottomOf="@+id/image"
            motion:layout_constraintStart_toStartOf="@+id/textView"
            motion:layout_constraintTop_toBottomOf="@+id/textView" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteY="16dp" />
        <Constraint
            motion:layout_constraintTop_toTopOf="parent"
            android:id="@+id/back"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_height="120dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent" />
        <Constraint
            android:id="@+id/back"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/back"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/image2"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintStart_toEndOf="@+id/arrow_back"
            motion:layout_constraintTop_toBottomOf="@+id/arrow_back"
            motion:transitionEasing="accelerate"
            android:alpha="1" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/back"
            android:layout_width="wrap_content"
            android:id="@+id/more"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/arrow_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            motion:layout_constraintBottom_toTopOf="@+id/textView2"
            motion:layout_constraintStart_toStartOf="@+id/textView2" />
        <Constraint
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/image"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/image"
            android:layout_width="match_parent"
            android:layout_height="360dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/image2"
            android:layout_width="match_parent"
            android:layout_height="360dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/back"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="360dp"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/more"
            motion:layout_constraintTop_toTopOf="@+id/back"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp" />
    </ConstraintSet>
</MotionScene>